создать фиб: число = функция(x) {
	если ( x == 0 ) {
		вернуть 0;
	}

	если ( x == 1 ) {
		вернуть 1;
	}

	вернуть фиб(x - 2) + фиб(x - 1);
};

создать рез: число = фиб(6);
вывести(-(рез-1));